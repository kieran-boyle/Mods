{
  "name": "configui",
  "version": "1.0.0",
  "description": "A simple UI for config.json",
  "main": "main.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "postinstall": "electron-builder install-app-deps",
    "start": "npm install && electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "author": "kiki",
  "license": "MIT",
  "devDependencies": {
    "ajv": "^6.5.1",
    "electron": "^22.2.0",
    "electron-builder": "^23.6.0"
  },
  "dependencies": {
    "yarn": "^1.22.19"
  },
  "build": {
    "productName": "Config",
    "appId": "scripter.Config",
    "win": {
      "target": [
        "portable"
      ]
    },
    "portable": {
      "artifactName": "config.exe"
    },
    "directories": {
      "output": "electron/output",
      "app": "./",
      "buildResources": "electron/buildResources"
    }
  }
}
